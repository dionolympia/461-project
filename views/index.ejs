      <div class="col-lg-12">
        <h1 class="text-center display-1">SongSearch.<strong>js</strong></h1>
      </div>
    </div>

    <br />
    <br />
    <br />

    <div class="row">
      <div class="col-lg-4 text-center bg-light rounded-pill p-5 mb-1 page-button">
        <a href="/titlepage" title="title">
          <img src="/images/title.png" alt="title" width="150" height="150" />
        </a>
      </div>
      <div class="col-lg-4 text-center bg-light rounded-pill p-5 mb-1 page-button">
        <a href="/artistpage" title="artist">
